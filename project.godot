config_version=4

[application]

config/name="Magicmix"
run/main_scene="res://menu/main_menu/main_menu.tscn"
boot_splash/fullsize=false
boot_splash/use_filter=false
boot_splash/bg_color=Color( 0, 0, 0, 1 )

[autoload]

errors="*res://scripts/errors.gd"
themes="*res://scripts/themes.gd"
game_settings="*res://scripts/settings/game_settings.gd"
